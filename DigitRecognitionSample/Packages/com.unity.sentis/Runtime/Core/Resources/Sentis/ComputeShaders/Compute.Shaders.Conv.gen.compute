    [unroll]
    for (bx = 0; bx < BLOCK_SIZE_X; bx++)
    {
        uint oy = baseOutputY + 4 * THREAD_COUNT_Y;
        uint ox = baseOutputX + bx * THREAD_COUNT_X;
        uint storeIndex = 0xFFFFFFFF;
        if (oy < outputChannels && ox < outputSize)
